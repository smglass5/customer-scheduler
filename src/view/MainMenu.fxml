<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefWidth="400.0" styleClass="background-pane" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="120.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="342.4" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Label styleClass="title" text="Main Menu" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="handleCustomerButton" mnemonicParsing="false" onAction="#handleCustomer" text="Customer" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
      <Button fx:id="handleAppointmentButton" minWidth="-Infinity" mnemonicParsing="false" onAction="#handleAppointment" prefHeight="30.0" prefWidth="120.0" text="Appointment" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="TOP" />
      <Button fx:id="handleReportButton" mnemonicParsing="false" onAction="#handleReport" text="Report" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="TOP" />
      <Button fx:id="handleExitButton" mnemonicParsing="false" onAction="#handleExit" text="Exit" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="TOP" />
   </children>
</GridPane>
